<UserControl x:Class="TA.Desktop.Views.NavigationBar"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800" Background="Transparent">
    
    <Grid Margin="2">

        <StackPanel Orientation="Vertical">
            <RadioButton Style="{StaticResource NavHomeButton}" IsChecked="True" Margin="5"/>
            <RadioButton Style="{StaticResource NavCompleteButton}" Margin="5"/>
            <RadioButton Style="{StaticResource NavAddButton}" Margin="5"/>
        </StackPanel>
            
    </Grid>
    
</UserControl>
